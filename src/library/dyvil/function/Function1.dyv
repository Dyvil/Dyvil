package dyvil.function

@FunctionalInterface
public interface Function1[-P1, +R]
{
	func apply(P1 par1): R

	infix func andThen[A, B, C](A => B first, B => C second): A => C = x => second(first(x))

	infix func compose[A, B, C](B => C second, A => B first): A => C = second andThen first

	static func identity[T]: Function1[T, T] = x => x
}
