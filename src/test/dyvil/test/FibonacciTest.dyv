package dyvil.test

public class FibonacciTest
{
	public static void main([String] args)
	{
		List[int] sequence = fibonacciSequence(20)

		for (int i <- 0 ..< sequence.size)
		{
			Main.test("Fibonacci #\(i + 1)", sequence[i], fibonacci(i))
		}
	}

	public static List[int] fibonacciSequence(int n)
	{
		MutableList[int] list = [ 1, 1 ]

		for (int i <- 2 ..< n)
		{
			list += list[i - 1] + list[i - 2]
		}

		return list
	}

	@dyvil.annotation.pure
	public static int fibonacci(int n)
	{
		if (n <= 1) return 1
		else		return fibonacci(n - 1) + fibonacci(n - 2)
	}
}
